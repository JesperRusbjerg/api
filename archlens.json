{
    "$schema": "https://raw.githubusercontent.com/Perlten/Architectural-Lens/master/config.schema.json",
    "name": "api",
    "rootFolder": "zeeguu",
    "github": {
        "url": "https://github.com/zeeguu/api",
        "branch": "master"
    },
    "saveLocation": "./diagrams/",
    "views": {
        "completeView": {
            "packages": [],
            "ignorePackages": []
        },"apiView": {
            "packages": ["api"],
            "ignorePackages": []
        },"coreView": {
            "packages": [
                "core"

            ],
            "ignorePackages": ["*test*"]
        },
        "languageAndWordView": {
            "packages": [
                "core.language",
                "core.model.word_knowledge"
            ],
            "ignorePackages": []
        },
        "topLevel": {
            "packages": [
                {
                    "packagePath": "",
                    "depth": 1
                }
            ],
            "ignorePackages": []
        }
    }
}